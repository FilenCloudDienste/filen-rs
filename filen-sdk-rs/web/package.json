{
	"name": "@filen/sdk-rs",
	"version": "0.3.39",
	"description": "Filen JS SDK using filen-sdk-rs",
	"type": "module",
	"scripts": {
		"build": "vite build",
		"test": "npm run build && vitest run --browser.name=chromium && vitest run --browser.name=firefox",
		"ubrn:ios": "ubrn build ios --and-generate",
		"ubrn:android": "ubrn build android --and-generate",
		"ubrn:checkout": "ubrn checkout",
		"ubrn:release": "ubrn build ios --release --and-generate && ubrn build android --release --and-generate"
	},
	"homepage": "https://github.com/FilenCloudDienste/filen-rs",
	"author": "Filen",
	"collaborators": [
		"Enduriel <endur1el@protonmail.com>"
	],
	"files": [
		"sdk-rs_bg.wasm",
		"sdk-rs_bg.wasm.d.ts",
		"sdk-rs.d.ts",
		"sdk-rs.js",
		"snippets",
		"service-worker",
		"filen-sdk-worker-thread.js",
		"android/src/main/",
		"android/build.gradle",
		"android/CMakeLists.txt",
		"android/cpp-adapter.cpp",
		"cpp",
		"ios",
		"FilenSdkRsFramework.xcframework",
		"src",
		"SdkRs.podspec"
	],
	"main": "./sdk-rs.js",
	"react-native": "./src/index.tsx",
	"exports": {
		".": {
			"react-native": {
				"android": {
					"import": "./src/index.tsx",
					"require": "./src/index.tsx",
					"types": "./src/index.tsx",
					"default": "./src/index.tsx"
				},
				"ios": {
					"import": "./src/index.tsx",
					"require": "./src/index.tsx",
					"types": "./src/index.tsx",
					"default": "./src/index.tsx"
				},
				"import": "./src/index.tsx",
				"require": "./src/index.tsx",
				"types": "./src/index.tsx",
				"default": "./src/index.tsx"
			},
			"browser": {
				"import": "./sdk-rs.js",
				"types": "./sdk-rs.d.ts"
			},
			"types": "./sdk-rs.d.ts",
			"import": "./sdk-rs.js",
			"default": "./sdk-rs.js"
		},
		"./sdk-rs_bg.wasm": {
			"import": "./sdk-rs_bg.wasm",
			"default": "./sdk-rs_bg.wasm"
		},
		"./service-worker/sdk-rs.js": "./service-worker/sdk-rs.js",
		"./service-worker/sdk-rs.d.ts": "./service-worker/sdk-rs.d.ts",
		"./service-worker/sdk-rs_bg.wasm": "./service-worker/sdk-rs_bg.wasm"
	},
	"sideEffects": [
		"./sdk-rs.js"
	],
	"license": "AGPL-3.0-only",
	"keywords": [
		"filen"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/FilenCloudDienste/filen-rs"
	},
	"engines": {
		"node": ">=23"
	},
	"devDependencies": {
		"@types/node": "^24.10.1",
		"@types/serviceworker": "^0.0.167",
		"@vitest/browser": "^4.0.14",
		"@vitest/browser-playwright": "^4.0.14",
		"@zip.js/zip.js": "^2.8.11",
		"playwright": "^1.57.0",
		"prettier": "^3.8.1",
		"typescript": "~5.9.3",
		"vite": "^7.2.4",
		"vite-plugin-node-polyfills": "^0.24.0",
		"vite-plugin-pwa": "^1.2.0",
		"vite-plugin-top-level-await": "^1.6.0",
		"vite-plugin-wasm": "^3.5.0",
		"vitest": "^4.0.14"
	},
	"codegenConfig": {
		"name": "FilenSdkRsTurboModule",
		"type": "modules",
		"jsSrcsDir": "src",
		"android": {
			"javaPackageName": "io.filen.sdkrsturbomodule"
		}
	},
	"dependencies": {
		"uniffi-bindgen-react-native": "github:Enduriel/uniffi-bindgen-react-native#e0282ad3eb972eaea74225dcc06247b7a67204dd"
	}
}
