{
  "name": "@filen/sdk-rs",
  "version": "0.3.23",
  "description": "Filen JS SDK using filen-sdk-rs",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "ubrn:ios": "ubrn build ios --and-generate",
    "ubrn:android": "ubrn build android --and-generate",
    "ubrn:checkout": "ubrn checkout",
    "ubrn:release": "ubrn build ios --release --and-generate && ubrn build android --release"
  },
  "homepage": "https://github.com/FilenCloudDienste/filen-rs",
  "author": "Filen",
  "collaborators": [
    "Enduriel <endur1el@protonmail.com>"
  ],
  "files": [
    "sdk-rs_bg.wasm",
    "sdk-rs_bg.wasm.d.ts",
    "sdk-rs.d.ts",
    "sdk-rs.js",
    "snippets",
    "filen-sdk-worker-thread.js",
    "android/src/main/",
    "android/build.gradle",
    "android/CMakeLists.txt",
    "android/cpp-adapter.cpp",
    "cpp",
    "ios",
    "FilenSdkRsFramework.xcframework",
    "src",
    "SdkRs.podspec"
  ],
  "main": "./sdk-rs.js",
  "react-native": "./src/index.tsx",
  "exports": {
    ".": {
      "react-native": {
        "source": "./src/index.tsx",
        "types": "./src/index.tsx"
      },
      "browser": {
        "import": "./sdk-rs.js",
        "types": "./sdk-rs.d.ts"
      },
      "types": "./sdk-rs.d.ts",
      "default": "./sdk-rs.js"
    },
    "./sdk-rs_bg.wasm": "./sdk-rs_bg.wasm"
  },
  "sideEffects": [
    "./sdk-rs.js"
  ],
  "license": "AGPL-3.0-only",
  "keywords": [
    "filen"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FilenCloudDienste/filen-rs"
  },
  "engines": {
    "node": ">=23"
  },
  "devDependencies": {
    "@vitest/browser": "^3.2.4",
    "@zip.js/zip.js": "^2.7.72",
    "playwright": "^1.56.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^3.2.4"
  },
  "codegenConfig": {
    "name": "FilenSdkRsTurboModule",
    "type": "modules",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "io.filen.sdkrsturbomodule"
    }
  },
  "dependencies": {
    "uniffi-bindgen-react-native": "github:Enduriel/uniffi-bindgen-react-native#430b88aca9d09d0f02f7871a34c123e64ed4f0ef"
  }
}
